{
  "name": "ui",
  "version": "1.0.4-8",
  "scripts": {
    "ng": "./node_modules/@angular/cli/bin/ng",
    "start": "npm run gen_versions_json;  cp deploy/env-local.js src/env.js;  ./node_modules/@angular/cli/bin/ng serve",
    "start-hotreload": "./node_modules/@angular/cli/bin/ng serve --live-reload",
    "build": "./node_modules/@angular/cli/bin/ng build",
    "build-prod": "npm run gen_versions_json;  ./node_modules/@angular/cli/bin/ng build --prod",
    "test": "./node_modules/@angular/cli/bin/ng test",
    "test:ci": "./node_modules/@angular/cli/bin/ng test --no-watch --no-progress --karma-config=karma.ci.conf.js",
    "lint": "./node_modules/@angular/cli/bin/ng lint",
    "e2e": "./node_modules/@angular/cli/bin/ng e2e",
    "clean": "rm -rf dist; rm -rf coverage; rm -rf reports; rm -rf test-results; rm -rf node_modules; rm -rf src/env.js",
    "gen_versions_json": "echo \"{ \\\"OSMT-UI\\\": { \\\"version\\\": \\\"$npm_package_version\\\" } }\" > src/assets/versions.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.16",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/forms": "~12.2.16",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "@concentricsky/wgu-design-system-patternlibrary": "13.0.0",
    "@ng-idle/core": "^11.0.0",
    "@ng-idle/keepalive": "^11.0.0",
    "@popperjs/core": "^2.6.0",
    "@types/chroma-js": "^2.1.3",
    "@types/file-saver": "^2.0.1",
    "chroma-js": "^2.1.0",
    "file-saver": "^2.0.2",
    "inert-polyfill": "^0.2.5",
    "ngx-papaparse": "^5.0.0",
    "rxjs": "~6.5.5",
    "ssri": "^8.0.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.17",
    "@angular/cli": "~12.2.17",
    "@angular/compiler-cli": "~12.2.16",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-sonarqube-unit-reporter": "^0.0.23",
    "lodash": "^4.17.21",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
